module Test {
  fun test : Number {
    0 * 10
  }
}
----------------------------------------------------OperationNumericTypeMismatch
module Test {
  fun test : Number {
    "Hello" * 0
  }
}
----------------------------------------------------OperationNumericTypeMismatch
module Test {
  fun test : Number {
    0 * "Hello"
  }
}
----------------------------------------------------OperationNumericTypeMismatch
