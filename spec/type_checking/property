component Main {
  property name : String = "Joe"

  render : Html {
    <div/>
  }
}
------------------------------------------------------------PropertyTypeMismatch
component Main {
  property name : String = true

  render : Html {
    <div/>
  }
}

-------------------------------------------------------PropertyWithTypeVariables
component Main {
  property name : Test(a) = true

  render : Html {
    <div/>
  }
}
-------------------------------------------------------PropertyWithTypeVariables
component Main {
  property name = []

  render : Html {
    <div/>
  }
}
------------------------------------------------------------PropertyTypeMismatch
component Main {
  property name : Test = { a = "Hello" }

  render : Html {
    <div/>
  }
}

