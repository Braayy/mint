module Result {
  error (input : a) : Result(a, b) {
    `new Err(input)`
  }
}

component Main {
  render : String {
    try {
      x =
        Result.error("Blah")

      x
    } catch String => error {
      error
    }
  }
}
------------------------------------------------------------------TryDidNotCatch
module Result {
  error (input : a) : Result(a, b) {
    `new Err(input)`
  }
}

component Main {
  render : String {
    try {
      x =
        Result.error("Blah")

      x
    }
  }
}
