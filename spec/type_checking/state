component Main {
  state b : String = "Hello"
  state a : Number = 0

  render : Html {
    <div/>
  }
}
---------------------------------------------------------------StateTypeMismatch
component Main {
  state b : String = 0

  render : Html {
    <div/>
  }
}
---------------------------------------------------------------StateTypeMismatch
component Main {
  state name : Test = { a = "Hello" }

  render : Html {
    <div/>
  }
}
