enum A {
  B
  C
}

component Main {
  fun y : A {
    A::B
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------------EnumIdTypeMismatch
enum A {
  B(String)
  C
}

component Main {
  fun y : A {
    A::B(0)
  }

  fun render : String {
    ""
  }
}
---------------------------------------------------------------EnumIdEnumMissing
enum A {
  B(String)
  C
}

component Main {
  fun y : A {
    A::D(0)
  }

  fun render : String {
    ""
  }
}
---------------------------------------------------------------EnumIdTypeMissing
component Main {
  fun y : A {
    C::D(0)
  }

  fun render : String {
    ""
  }
}
