enum A {
  B
  C
}

component Main {
  render : String {
    case (A::B) {
      => ""
    }
  }
}
--------------------------------------------------------------EnumIdTypeMismatch
enum A {
  B(String)
  C
}

component Main {
  render : String {
    case (A::B(0)) {
      => ""
    }
  }
}
---------------------------------------------------------------EnumIdEnumMissing
enum A {
  B(String)
  C
}

component Main {
  render : String {
    case (A::D(0)) {
      => ""
    }
  }
}
---------------------------------------------------------------EnumIdTypeMissing
component Main {
  render : String {
    case (C::D(0)) {
      => ""
    }
  }
}
