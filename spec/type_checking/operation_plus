component Main {
  fun test : String {
    "Hello" + "There"
  }

  fun render : String {
    ""
  }
}
-------------------------------------------------------OperationPlusTypeMismatch
component Main {
  fun test : Bool {
    "Hello" + void
  }

  fun render : String {
    ""
  }
}
