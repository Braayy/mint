record Test {
  a : String,
  b : Number
}

component Main {
  test : Test {
    {
      a = "Hello",
      b = 0
    }
  }

  render : String {
    ""
  }
}
-----------------------------------------------------------------RecordWithHoles
record Test {
  a : Array(a),
  b : Number
}
--------------------------------------------------------------------------------
component Main {
  test : Object {
    encode {
      a = "Hello",
      b = "Blah"
    }
  }

  render : String {
    ""
  }
}
--------------------------------------------------------------------------------
component Main {
  test : Object {
    encode {
      a = "Hello",
      b = {
        c = "Blah"
      }
    }
  }

  render : String {
    ""
  }
}
