component Main {
  test : Array(String) {
    [
      "Hello",
      "Blah",
      "Joe"
    ]
  }

  render : Html {
    try {
      test()

      <div/>
    }
  }
}
-----------------------------------------------------------------ArrayNotMatches
component Main {
  test : Array(String) {
    [
      "Hello",
      true,
      "Joe"
    ]
  }

  render : Html {
    try {
      test()

      <div/>
    }
  }
}
------------------------------------------------------ArrayNotMatchesDefinedType
component Main {
  test : Array(String) {
    [
      "Hello",
      "Joe"
    ] of Number
  }

  render : Html {
    try {
      test()

      <div/>
    }
  }
}
