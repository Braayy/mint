component Main {
  test : String {
    a()
  } where {
    a = () : String { "Hello" }
  }

  render : String {
    ""
  }
}
------------------------------------------------------------FunctionTypeMismatch
component Main {
  test : String {
    a()
  } where {
    a = () : Bool { true }
  }

  render : String {
    ""
  }
}
------------------------------------------------------InlineFunctionTypeMismatch
component Main {
  test : String {
    a()
  } where {
    a = () : String { true }
  }

  render : String {
    ""
  }
}
