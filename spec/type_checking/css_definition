component A {
  style test {
    color: {"blue"};
    color: {color};
    color: red;
    color: {0};
    top: {top};
  }

  get color : String {
    "blue"
  }

  get top : Number {
    0
  }

  fun render : Html {
    <div/>
  }
}
-------------------------------------------------------CssDefinitionTypeMismatch
component A {
  style test {
    color: {true};
  }

  fun render : Html {
    <div/>
  }
}
