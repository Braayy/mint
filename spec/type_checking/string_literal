component Main {
  render : String {
    "Hello There"
  }
}
--------------------------------------------------------------------------------
component Main {
  render : String {
    try {
      name = 0

      "Hello There #{name}!"
    }
  }
}
------------------------------------------StringLiteralInterpolationTypeMismatch
component Main {
  render : String {
    try {
      name = {}

      "Hello There #{name}!"
    }
  }
}
