store A {
  get test : Bool {
    x
  } where {
    x =
      true
  }
}

component Main {
  connect A exposing { test }

  render : String {
    ""
  }
}
-----------------------------------------------------------------GetTypeMismatch
store A {
  get test : Bool {
    x
  } where {
    x =
      "hello"
  }
}

component Main {
  connect A exposing { test }

  render : String {
    ""
  }
}
