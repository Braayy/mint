module Test {
  a (value : String, x : Bool) : String {
    value
  }

  b : String {
    true
    |> Test.a("test")
  }
}

component Main {
  render : String {
    Test.b()
  }
}
--------------------------------------------------------------------------------
global component Test {
  b : String {
    ""
  }

  render : Html {
    <div></div>
  }
}

component Main {
  render : String {
    Test.b()
  }
}
------------------------------------------------------ModuleAccessNotFoundModule
module Test {
  b : String {
    X.x()
  }
}

component Main {
  render : String {
    Test.b()
  }
}
----------------------------------------------------ModuleAccessNotFoundFunction
module Test {
  b : String {
    Test.a("test")
  }
}

component Main {
  render : String {
    Test.b()
  }
}
--------------------------------------------------------------------------------
module Test {
  a (value : String, x : Bool) : String {
    value
  }

  b : Function(Bool, String) {
    Test.a("test")
  }
}

component Main {
  render : String {
    try {
      z = Test.b()

      z(true)
    }
  }
}
--------------------------------------------------------CallArgumentTypeMismatch
module Test {
  a (value : String, x : Bool) : String {
    value
  }

  b : String {
    Test.a("test", "z")
  }
}

component Main {
  render : String {
    Test.b()
  }
}
--------------------------------------------------------CallArgumentTypeMismatch
module Test {
  a (value : a, x : a) : String {
    value
  }

  b : String {
    Test.a("test", 0)
  }
}

component Main {
  render : String {
    Test.b()
  }
}
