component Main {
  get a : String {
    "Hello"
  }

  b : String {
    a
  }

  render : String {
    b()
  }
}
-----------------------------------------------------------------VariableMissing
component Main {
  get a : String {
    "Hello"
  }

  b : String {
    c
  }

  render : Html {
    b()
  }
}
----------------------------------------------------------------VariableReserved
component Main {
  b(default : String) : String {
    default
  }

  render : Html {
    b("")
  }
}
