record X {
  name : String
}

component Main {
  render : Html {
    try {
      decode (`{}`) as X

      <div/>
    } catch {
      <div/>
    }
  }
}
------------------------------------------------------------DecodeExpectedObject
record X {
  name : String
}

component Main {
  render : Html {
    try {
      decode "" as X

      <div/>
    }
  }
}
---------------------------------------------------------------DecodeComplexType
record X {
  name : Blah
}

component Main {
  render : Html {
    try {
      decode (`{}`) as X

      <div/>
    }
  }
}
---------------------------------------------------------------DecodeComplexType
record X {
  name : Maybe(Blah)
}

component Main {
  render : Html {
    try {
      decode (`{}`) as X

      <div/>
    }
  }
}
