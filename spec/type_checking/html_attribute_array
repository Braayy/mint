component Main {
  property things : Array(String) = []

  render : Html {
    <Main things=["x"]/>
  }
}
--------------------------------------HtmlAttributeComponentPropertyTypeMismatch
component Main {
  property things : Array(String) = []

  render : Html {
    <Main things=[0]/>
  }
}
---------------------------------------HtmlAttributeElementAttributeTypeMismatch
component Main {
  render : Html {
    <div things=[0]/>
  }
}
