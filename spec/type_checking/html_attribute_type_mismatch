component X {
  property test : String = ""

  render : Html {
    <div/>
  }
}

component Main {
  render : Html {
    <X key="wtf" test="asd"/>
  }
}
--------------------------------------HtmlAttributeComponentPropertyTypeMismatch
component X {
  property test : String = ""

  render : Html {
    <div/>
  }
}

component Main {
  render : Html {
    <X key="wtf" test={0}/>
  }
}
