component Main {
  render : String {
    try {
      x = "hello"
    }
  }
}
---------------------------------------------------------------StatementNotTuple
component Main {
  render : String {
    try {
      {x, y, z} = "hello"
    }
  }
}
----------------------------------------------------------StatementTupleMismatch
component Main {
  render : String {
    try {
      {x, y, z} = {"hello", "a"}
    }
  }
}
