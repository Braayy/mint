module Test {
  test : Bool {
    !false
  }
}

component Main {
  render : String {
    if (Test.test()) {
      "Hello"
    } else {
      "Hello"
    }
  }
}
--------------------------------------------------------NegatedExpressionNotBool
module Test {
  test : Bool {
    !"asd"
  }
}

component Main {
  render : String {
    if (Test.test()) {
      "Hello"
    } else {
      "Hello"
    }
  }
}
