component Main {
  fun function : Bool {
    true
  }

  fun render : String {
    ""
  }
}
------------------------------------------------------------FunctionTypeMismatch
component Main {
  fun function : Bool {
    "hello"
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------FunctionArgumentConflict
component Main {
  fun function (a : String, a : String) : Bool {
    "hello"
  }

  fun render : String {
    ""
  }
}
