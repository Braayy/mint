module Result {
  error (input : a) : Result(a, b) {
    `new Err(input)`
  }
}

module A {
  test : String {
    try{x=Result.error("hello") y=Result.error(0)}catch String => a {"blah"}catch{"X"}
  }
}
--------------------------------------------------------------------------------
module Result {
  error (input : a) : Result(a, b) {
    `new Err(input)`
  }
}

module A {
  test : String {
    try {
      x =
        Result.error("hello")

      y =
        Result.error(0)
    } catch String => a {
      "blah"
    } catch {
      "X"
    }
  }
}
