record A {
  a : String,
  b : Bool
}

module Test {
  test : A {
    {x|a="Hello",b=true}
  } where {
    x =
      { a = "Blah", b = false }
  }
}
--------------------------------------------------------------------------------
record A {
  a : String,
  b : Bool
}

module Test {
  test : A {
    { x |
      a = "Hello",
      b = true
    }
  } where {
    x =
      {
        a = "Blah",
        b = false
      }
  }
}
