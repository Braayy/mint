record Blah1 {
  blah : Bool
}

record Blah {
  blah : Blah1
}

module Test {
  test : Bool {
    blah.blah.blah
  } where {
    blah =
      { blah = { blah = true } }
  }
}
--------------------------------------------------------------------------------
record Blah1 {
  blah : Bool
}

