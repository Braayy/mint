module Test {
  fun test : Bool {
    if ("aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa" \
        "aaaaaa" == "b") {
      true
    } else {
      false
    }
  }
}
