component Main {
  fun test : String {
    a("Joe")
  } where {
    a =
      (b : String) : String => { b }
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    let a = ((b) => {
      return b
    })

    return a(`Joe`)
  }

  render() {
    return ``
  }
}

$Main.displayName = "Main"
