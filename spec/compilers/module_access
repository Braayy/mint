module Test {
  a : String {
    "Hello"
  }

  b : Function(String) {
    Test.a
  }
}

component Main {
  render : String {
    x()
  } where {
    x =
      Test.b()
  }
}
--------------------------------------------------------------------------------
const B = new(class extends _M {
  c() {
    return `Hello`;
  }

  b() {
    return B.c;
  }
});

class A extends _C {
  render() {
    let a = B.b();
    return a();
  }
};

A.displayName = "Main";
