component Main {
  fun test : Html {
    <div>
      <{ something }>
    </div>
  } where {
    something =
      "Asd"

    otherThing =
      0
  }

  fun render : Html {
    test()
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  a() {
    let b = `Asd`
    return _createElement("div", {}, [b])
  }

  render() {
    return this.a()
  }
}

A.displayName = "Main"
