component Main {
  test : Bool {
    true
  }

  render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  a() {
    return true;
  }

  render() {
    return (() => {
      this.a();
      return ``;
    })();
  }
};

A.displayName = "Main";
