component Main {
  property name : String = "Joe"

  fun render : Html {
    <div/>
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  constructor(props) {
    super(props)

    this.defaultProps = {
      a: `Joe`
    }
  }

  get a() {
    return 'a' in this.props ? this.props.a : this.defaultProps.a
  }

  render() {
    return _createElement("div", {})
  }
}

A.displayName = "Main"
