component Main {
  state background : String = "blue"

  get styles : Map(String, String) {
    ``
  }

  style test {
    background: #{background};
    color: red;
  }

  fun render : Html {
    <div::test style={styles}>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  constructor(props) {
    super(props);

    this.state = new Record({
      a: `blue`
    });
  }

  $foo-a() {
    const _ = {
      [`--a-a`]: this.a
    };

    return _;
  }

  get b() {
    return;
  }

  get a() {
    return this.state.a;
  }

  render() {
    return _h("div", {
      className: `foo-a`,
      style: _style([this.$foo-a(), this.b])
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.foo-a {
  background: var(--a-a);
  color: red;
}
`);
