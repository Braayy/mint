module X {
  fun a : Void {
    void
  }
}

component Main {
  fun test : Void {
    with X {
      a()
    }
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------------------------------
const $X = new(class {
  a() {
    return null
  }
})

class $Main extends Component {
  test() {
    return $X.a.bind($X)()
  }

  render() {
    return ``
  }
}

$Main.displayName = "Main"
