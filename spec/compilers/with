module SomeModule {
  fun someFunction : String {
    ""
  }
}

component Main {
  fun render : String {
    with SomeModule {
      someFunction()
    }
  }
}
--------------------------------------------------------------------------------
const B = new(class extends Module {
  a() {
    return ``
  }
})

class A extends Component {
  render() {
    return B.a()
  }
}

A.displayName = "Main"

