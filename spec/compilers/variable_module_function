module A {
  a : String {
    "Hello"
  }

  b : Function(String) {
    a
  }
}

component Main {
  render : String {
    a()
  } where {
    a = A.b()
  }
}
--------------------------------------------------------------------------------
const B = new(class extends _M {
  c() {
    return `Hello`;
  }

  b() {
    return B.c;
  }
});

class A extends _C {
  render() {
    let a = B.b();
    return a();
  }
};

A.displayName = "Main";

