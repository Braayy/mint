component If {
  fun test : Bool {
    if ("asd" == "asd2") {
      true
    } else {
      false
    }
  }

  fun render : Html {
    <div/>
  }
}
--------------------------------------------------------------------------------
class $If extends Component {
  test() {
    return (_compare(`asd`, `asd2`) ? true : false)
  }

  render() {
    return _createElement("div", {})
  }
}
