component Main {
  fun test : String {
    something
  } where {
    something =
      "Asd"

    otherThing =
      0
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    let something = `Asd`

    return something
  }

  render() {
    return ``
  }
}

$Main.displayName = "Main"

