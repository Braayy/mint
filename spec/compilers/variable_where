component Main {
  fun test : String {
    a
  } where {
    a =
      "Hello"
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    let a = `Hello`

    return a
  }

  render() {
    return ``
  }
}

$Main.displayName = "Main"

