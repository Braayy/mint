component Main {
  fun a : String {
    "test"
  }

  fun test : String {
    a()
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  a() {
    return `test`
  }

  b() {
    return this.a()
  }

  render() {
    return (() => {
      this.b()

      return ``
    })()
  }
}

A.displayName = "Main"
