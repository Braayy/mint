component Main {
  a : String {
    "test"
  }

  test : String {
    a()
  }

  render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  a() {
    return `test`;
  }

  b() {
    return this.a();
  }

  render() {
    return (() => {
      this.b();
      return ``;
    })();
  }
};

A.displayName = "Main";
