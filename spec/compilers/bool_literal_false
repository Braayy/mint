component Main {
  fun test : Bool {
    false
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  a() {
    return false
  }

  render() {
    return (() => {
      this.a()

      return ``
    })()
  }
}

A.displayName = "Main"
