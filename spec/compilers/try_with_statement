component Main {
  fun test : String {
    try {
      x =
        "hello"

      x
    }
  }

  fun render : String {
    ""
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    return (() => {
      let x = `hello`

      return x
    })()
  }

  render() {
    return ``
  }
}

$Main.displayName = "Main"
