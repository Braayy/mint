component Main {
  fun render : Html {
    <div>
      <>
        <{ "A" }>
        < key="something"></>
      </>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  render() {
    return _createElement("div", {}, [_createElement(React.Fragment, {}, [`A`, _createElement(React.Fragment, { "key": `something` }, [])])])
  }
}

A.displayName = "Main"
