store Test {
  get a : String {
    "Hello"
  }

  fun b : String {
    Test.a
  }
}

component Main {
  fun render : String {
    Test.b()
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  render() {
    return B.a()
  }
}

A.displayName = "Main"

const B = new(class extends Store {
  constructor() {
    super()

    this.state = {

    }
  }

  get b() {
    return `Hello`
  }

  a() {
    return B.b
  }
})
