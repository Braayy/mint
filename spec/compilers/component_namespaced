--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return `test`;
  }
};

A.displayName = "Ui.Dropdown";

class B extends _C {
  render() {
    return $a();
  }
};

B.displayName = "Main";

const $a = _m(() => _h(A, {}));
