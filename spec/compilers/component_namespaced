component Ui.Dropdown {
  fun render : String {
    "test"
  }
}

component Main {
  fun render : Html {
    <Ui.Dropdown/>
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  render() {
    return `test`
  }
}

A.displayName = "Ui.Dropdown"

class B extends Component {
  render() {
    return _createElement(A, {  })
  }
}

B.displayName = "Main"
