component Main {
  style one {
    color: red;
  }

  style two {
    color: blue;
  }

  fun render : Html {
    <div::one::two>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return _h("div", {
      className: `foo-a foo-b`
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.foo-a {
  color: red;
}

.foo-b {
  color: blue;
}
`);
