record X {
  name : String
}

component Main {
  fun render : String {
    x.name
  } where {
    x = { name = "test" }
  }
}
--------------------------------------------------------------------------------
const A = _createRecord({
  a: [
    "name",
    Decoder.string
  ]
})

class B extends _C {
  render() {
    let a = new A({
      a: `test`
    })

    return a.a
  }
}

B.displayName = "Main"
