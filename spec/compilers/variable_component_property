component Main {
  property test : String = "Hello"

  fun render : String {
    test
  }
}
--------------------------------------------------------------------------------
class A extends Component {
  constructor(props) {
    super(props)

    this.defaultProps = {
      a: `Hello`
    }
  }

  get a() {
    return 'a' in this.props ? this.props.a : this.defaultProps.a
  }

  render() {
    return this.a
  }
}

A.displayName = "Main"
