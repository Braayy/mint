store A {
  get test : String {
    something
  } where {
    something =
      "Asd"

    otherThing =
      0
  }
}
--------------------------------------------------------------------------------
const $A = new(class extends Store {
  constructor() {
    super()
    this.state = {

    }
  }

  get test() {
    let something = `Asd`

    let otherThing = 0

    return something
  }
})
$A.__displayName = `A`
