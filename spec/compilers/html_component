component Test {
  render : Html {
    <div/>
  }
}

component Main {
  render : Html {
    <Test></Test>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return _h("div", {});
  }
};

A.displayName = "Test";

class B extends _C {
  render() {
    return $a();
  }
};

B.displayName = "Main";

const $a = _m(() => _h(A, {}));
