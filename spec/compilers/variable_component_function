component A {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    a
  }

  fun render : Html {
    <div/>
  }
}
--------------------------------------------------------------------------------
class $A extends Component {
  a() {
    return `Hello`
  }

  b() {
    return this.a.bind(this)
  }

  render() {
    return _createElement("div", {})
  }
}
